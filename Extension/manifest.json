{
  "name": "Fix News",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Fix News"
  },
  "background":{
    "scripts": ["background.js"]
  },

  "permissions": ["tabs"]
}
